{
  "rewrites": [
    {
      "source": "/",
      "destination": "/holland_test_preview.html"
    },
    {
      "source": "/preview",
      "destination": "/holland_test_preview.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}


